            <tr ng-repeat="c in cdes">
                <td class="name">
                    <a href="#/deview/?tinyId={{c.tinyId}}">
                        {{c.primaryNameCopy}}
                    </a>
                </td>
                <td class="naming">
                    <ul>
                        <li ng-repeat="name in c.naming">{{name.designation}}</li>
                    </ul>
                </td>
                <td class="permissibleValues multiline-ellipsis">
                    <ul>
                        <li ng-repeat="pv in c.valueDomain.permissibleValues">
                            {{pv.valueMeaningName}}
                        </li>
                    </ul>
                </td>
                <td class="stewardOrg">{{c.stewardOrg.name}}</td>
                <td class="usedBy multiline-ellipsis">
                    <span ng-repeat="org in c.usedBy">{{$index>0?", ":""}}{{org}}</span>
                </td>
                <td class="registrationStatus">{{c.registrationState.registrationStatus}}</td>
                <td class="administrativeStatus">{{c.registrationState.administrativeStatus}}</td>
                <td class="ids">
                    <div ng-repeat="id in c.ids">
                        {{id.source}}: {{id.id}} v{{id.version}}
                    </div>
                </td>
            </tr>
        </tbody>
    </table>
