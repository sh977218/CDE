<div class="container-fluid">
    <h1 class="page-header">
        <label>User Profile</label>
    </h1>
    
    <h3>General Info</h3>
    <div class="row">
        <div class="col-md-4">
            <table class="table table-user-information">
                <tbody>
                    <tr>
                      <td>Username:</td>
                      <td>{{user.username}}</td>
                    </tr>
                    <tr>
                      <td>Quota:</td>
                      <td>{{user.quota / 1024 / 1024 | number:2 | placeholdEmpty}}<span ng-show="hasQuota"> MB</span></td>
                    </tr>
                    <tr>
                      <td>E-mail:</td>
                      <td><div inline-edit id="emailEdit" model="user.email" input-type="'email'" is-allowed="true" on-ok="saveProfile()" /></td>
                    </tr>
                    <tr>
                      <td>Curator for:</td>
                      <td></td>
                    </tr>
                    <tr>
                      <td>Admin for:</td>
                      <td></td>
                    </tr>
                    <tr>
                      <td></td>
                      <td></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    
