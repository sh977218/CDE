<div accordion close-others="false">
    <div accordion-group ng-repeat="cde in (cdes+mltCdes)" is-open="cde.isOpen">
        <div accordion-heading>
            <span ng-bind-html="cde.naming[0].designation" id="acc_link_{{$index}}" class="hand-cursor"></span>
            <i ng-show="ejsPage!='createcde'" class="fa fa-eye hand-cursor" ng-click="accordionIconAction(cde,'view',$event)" title="View"></i>
        </div>
        
        <div class="row" >
            <a ng-show="ejsPage!='createcde'" class='btn btn-small btn-default' ng-href="#/deview/?cdeId={{cde._id}}" title="Full Detail for {{cde.naming[0].designation}}"><i class="fa fa-eye"></i> View Full Detail</a>
        </div>        
    </div>
</div>