<div ng-controller="FormListCtrl">
    <div ng-repeat="form in listForms">
        <dl class="dl-horizontal">
            <dt>Name: </dt><dd>{{form.naming[0].designation}}</dd>
            <dt>Definition: </dt><dd>{{form.naming[0].definition}}</dd>
            <dt>Version: </dt><dd>{{form.version}}</dd>
            <dt>Steward: </dt><dd>{{form.stewardOrg.name}}</dd>
        </dl>        
    </div>    
</div>

