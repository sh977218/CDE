<!DOCTYPE html>
<html xmlns:ng="http://angularjs.org" lang="en">
<head>
    <base href="/">
    <title>NIH Common Data Elements (CDE) Repository</title>
    <meta name="site-version">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description"
          content="Repository of Common Data Elements (CDE) and Protocol Forms. Search CDEs. Search Protocol Forms.">
    <meta name="keywords"
          content="cde, common data element, form, protocol, protocol form, crf, case report form, promis, neuro-qol, phenx, ahrq, ninds, value set, repository, nih, nlm, national institutes of health, national library of medicine">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css"
          integrity="sha384-rwoIResjU2yc3z8GV/NPeZWAv56rSmLldC3R/AZzGRnGxQQKnKkoFVhFQhNUwEyJ" crossorigin="anonymous">
    <style>
        .card {
            padding: 5px;
            margin: 5px;
        }
    </style>
</head>
<body role="application">
<nav class="navbar navbar-toggleable-md navbar-inverse bg-inverse">
    <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarNav"
            aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <a class="navbar-brand">
        <img alt="Home" src="../../../cde/public/assets/img/nih-cde-logo-simple.png"/>
    </a>
    <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
            <li class="nav-item active">
                <a class="nav-link">CDEs</a>
            </li>
            <li class="nav-item">
                <a class="nav-link">Forms</a>
            </li>
            <li class="nav-item">
                <a class="nav-link">Boards</a>
            </li>
            <li class="nav-item">
                <a class="nav-link">Quick Board</a>
            </li>
            <li class="nav-item">
                <a class="nav-link">Help</a>
            </li>
        </ul>
    </div>
</nav>
<div class="container">
    <div class="row" id="general_tab">
        <h4 class="">General Details:</h4>
        <div class="clearfix"></div>
        <dl class="row">
            <dt class="col-3 text-right">Name:</dt>
            <dd class="col-9"><%= cde.naming[0].designation %></dd>
            <dt class="col-3 text-right">Steward:</dt>
            <dd class="col-9"><%= cde.stewardOrg.name %></dd>
            <dt class="col-3 text-right">Definition:</dt>
            <dd class="col-9"><%= cde.naming[0].definition %></dd>
            <dt class="col-3 text-right">Registration Status:</dt>
            <dd class="col-9"><%= cde.registrationState.registrationStatus %></dd>
        </dl>
    </div>
    <div class="row" id="pvs_tab">
        <h4>Permissible Values:</h4>
        <dl class="row">
            <dt class="col-3 text-right">Value Type:</dt>
            <dd class="col-9"><%= cde.valueDomain.datatype %></dd>
            <dt class="col-3 text-right">Unit of Measure:</dt>
            <dd class="col-9"><%= cde.valueDomain.uom %></dd>
            <dt class="col-3 text-right">Ids:</dt>
            <dd class="col-9"><%= cde.valueDomain.ids %></dd>
        </dl>
        <% if (cde.valueDomain.permissibleValues.length) { %>
        <table class="table table-striped">
            <thead>
            <tr>
                <th>Value</th>
                <th>Code Name</th>
                <th>Code</th>
                <th>Code System</th>
                <th>Code Description</th>
            </tr>
            </thead>
            <tbody>
            <% for(var i = 0; i < cde.valueDomain.permissibleValues.length; i++) {
                var pv = cde.valueDomain.permissibleValues[i]; %>
            <tr>
                <td><%= pv.permissibleValue %></td>
                <td><%= pv.valueMeaningName %></td>
                <td><%= pv.valueMeaningCode %></td>
                <td><%= pv.codeSystemName %></td>
                <td><%= pv.valueMeaningDefinition %></td>
            </tr>
            <% } %>
            </tbody>
        </table>
        <% } %>
    </div>
    <div class="row" id="naming_tab">
        <h4>Naming:</h4>
        <% for(var i = 0; i < cde.naming.length; i++) {
            var name = cde.naming[i];
        %>
        <dl class="row">
            <dt class="col-3 text-right">Name:</dt>
            <dd class="col-9"><%= name.designation %></dd>
            <dt class="col-3 text-right">Definition:</dt>
            <dd class="col-9"><%= name.definition %></dd>
            <dt class="col-3 text-right">Tags:</dt>
            <dd class="col-9"><%= name.tags %></dd>
        </dl>
        <% } %>
    </div>
    <div class="row" id="referenceDocuments_tab">
        <h4>Reference Documents:</h4>
        <% for(var i = 0; i < cde.referenceDocuments.length; i++) {
            var referenceDocument = cde.referenceDocuments[i];
        %>
        <dl class="row">
            <dt class="col-3 text-right">ID:</dt>
            <dd class="col-9"><%= referenceDocument.referenceDocumentId %></dd>
            <dt class="col-3 text-right">Title:</dt>
            <dd class="col-9"><%= referenceDocument.title %></dd>
            <dt class="col-3 text-right">URI:</dt>
            <dd class="col-9"><%= referenceDocument.uri %></dd>
            <dt class="col-3 text-right">Provider Org:</dt>
            <dd class="col-9"><%= referenceDocument.providerOrg %></dd>
            <dt class="col-3 text-right">Language Code:</dt>
            <dd class="col-9"><%= referenceDocument.languageCode %></dd>
            <dt class="col-3 text-right">Document:</dt>
            <dd class="col-9"><%= referenceDocument.document %></dd>
        </dl>
        <% } %>
    </div>
    <div class="row" id="properties_tab">
        <h4>Properties:</h4>
        <% for(var i = 0; i < cde.properties.length; i++) {
            var property = cde.properties[i];
        %>
        <dl class="row">
            <dt class="col-3 text-right">Key:</dt>
            <dd class="col-9"><%= property.key %></dd>
            <dt class="col-3 text-right">Value:</dt>
            <% if (property.valueFormat === 'html') { %>
            <dd class="col-9"><%- property.value %></dd>
            <% } %>
            <% if (property.valueFormat !== 'html') { %>
            <dd class="col-9"><%= property.value %></dd>
            <% } %>
        </dl>
        <% } %>
    </div>
    <div class="row" id="ids_tab">
        <h4>Identifiers:</h4>
        <dl class="row">
            <dt class="col-3 text-right">Source:</dt>
            <dd class="col-9">NLM</dd>
            <dt class="col-3 text-right">Id:</dt>
            <dd class="col-9"><%= cde.tinyId %></dd>
            <dt class="col-3 text-right">Version:</dt>
            <dd class="col-9"><%= cde.version %></dd>
        </dl>
        <% for(var i = 0; i < cde.ids.length; i++) {
            var id = cde.ids[i];
        %>
        <dl class="row">
            <dt class="col-3 text-right">Source:</dt>
            <dd class="col-9"><%= id.source %></dd>
            <dt class="col-3 text-right">Id:</dt>
            <dd class="col-9"><%= id.id %></dd>
            <dt class="col-3 text-right">Version:</dt>
            <dd class="col-9"><%= id.version %></dd>
        </dl>
        <% } %>
    </div>
</div>
