<body role="main" id="top">

<style>
    #spinner {
        width: 100px;
        height: 100px;
        border-radius: 50%;
        background-color: transparent;
        margin: 20% auto;
        border: 8px solid transparent;
        border-top: 8px solid rgb(0, 27, 192);
        border-left: 8px solid rgba(238, 238, 238, 0.4);
        border-bottom: 8px solid rgb(0, 27, 192);
        border-right: 8px solid rgba(238, 238, 238, 0.4);
        animation: shadowspin .6s linear infinite;
        -webkit-animation: shadowspin .6s linear infinite;
    }

    @keyframes spin {
        from {
            transform: rotate(0deg);
        }
        to {
            transform: rotate(360deg);
        }
    }

    @-moz-keyframes spin {
        from {
            -moz-transform: rotate(0deg);
        }
        to {
            -moz-transform: rotate(360deg);
        }
    }

    @-webkit-keyframes spin {
        from {
            -webkit-transform: rotate(0deg);
        }
        to {
            -webkit-transform: rotate(360deg);
        }
    }

    @keyframes shadowspin {
        0% {
            transform: rotate(0deg);
            box-shadow: inset 0 0 15px rgb(0, 27, 192);
        }
        55% {
            box-shadow: inset 0 0 35px rgba(238, 238, 238, 0.4);
        }

        100% {
            transform: rotate(360deg);
            box-shadow: inset 0 0 15px rgb(0, 27, 192);
        }
    }

    @-moz-keyframes shadowspin {
        0% {
            -moz-transform: rotate(0deg);
            box-shadow: inset 0 0 15px rgb(0, 27, 192);
        }

        55% {
            box-shadow: inset 0 0 35px rgba(238, 238, 238, 0.4);
        }

        100% {
            -moz-transform: rotate(360deg);
            box-shadow: inset 0 0 15px rgb(0, 27, 192);
        }
    }

    @-webkit-keyframes shadowspin {
        0% {
            -webkit-transform: rotate(0deg);
            box-shadow: inset 0 0 25px rgb(0, 27, 192);
        }

        55% {
            box-shadow: inset 0 0 5px rgba(238, 238, 238, 0.4);
        }

        100% {
            -webkit-transform: rotate(360deg);
            box-shadow: inset 0 0 25px rgb(0, 27, 192);
        }
    }
</style>
<div itemscope>

    <%- include googleAnalytics.html %>
    <nih-cde></nih-cde>

    <div class="loading" id="spinner">
    </div>

    <%- include footer.html %>

</div>

<!-- Polyfills for legacy browsers -->
<!-- Apply to all IE -->
<script type="text/javascript">
    (function () {
        function CustomEvent(event, params) {
            params = params || {bubbles: false, cancelable: false, detail: undefined};
            var evt = document.createEvent('CustomEvent');
            evt.initCustomEvent(event, params.bubbles, params.cancelable, params.detail);
            return evt;
        }

        CustomEvent.prototype = window.Event.prototype;
        if (typeof(window.Event) !== 'function')
            window.Event = CustomEvent;
    })();
</script>

