    <script type="text/javascript">
        var d = document.createElement('link');
        d.href = '/app/cde.css';
        d.rel = 'stylesheet';
        d.type = 'text/css';
        var l = document.getElementsByTagName('meta')[0];
        l.parentNode.insertBefore(d, l);

        d = document.createElement('link');
        d.href = 'https://fonts.googleapis.com/icon?family=Material+Icons';
        d.rel = 'stylesheet';
        d.type = 'text/css';
        l = document.getElementsByTagName('meta')[0];
        l.parentNode.insertBefore(d, l);
    </script>
    <noscript>
        <!-- build:css /app/cde.css -->
        <link rel="stylesheet" type="text/css" href="/app/cde.css" media="all"/>
        <!-- endbuild -->

        <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    </noscript>
</head>

<body role="main" id="top">

