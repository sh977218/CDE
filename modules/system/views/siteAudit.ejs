<div tabset class="pt40 pb40 container">
    <div tab heading="Logs">
        <%- include logAudit.ejs %>
    </div>
    <div tab heading="Usage">
        <%- include dailyUsage.ejs %>
    </div>
    <div tab heading="Server Errors">
        <div class="table" ng-controller="AuditServerErrorListCtrl">
            <div ng-include="'/system/public/html/errorList.html'"></div>
        </div>
    </div>    
    <div tab heading="Client Errors">
        <div class="table" ng-controller="AuditClientErrorListCtrl">
            <div ng-include="'/system/public/html/errorList.html'"></div>
        </div>
    </div>     

    <% for (module in config.modules) { if (!config.modules[module].audit) continue; %>
        <div tab heading="CDE Audit Log">
            <div class="table" ng-controller="AuditLogCtrl">
                <div ng-include="'/system/public/html/auditLog.html'" ng-init="module='<%= module %>'"></div>
            </div>
        </div>    
    <% } %>
      
</div>