<% if (config.webtrends) { %>
    <script src="/system/public/js/webtrends.js" type="text/javascript"></script>
    <script type="text/javascript">
        var _tag=new WebTrends();

        function submitWebtrends(){
            _tag.DCS.dcsuri = window.location.hash;
            dcsMultiTrack(); // dcsCollect() is the original function in the script, but this was recommended by the dev. community
        };

        _tag.dcsGetId();
        _tag.dcsCustom=function(){
            _tag.DCSext.dcsid=_tag.dcsid
            _tag.DCS.dcsuri = window.location.hash; // Angular Route Tracking, Track All Events must be set to FALSE!
        }
        _tag.trackAllEvents=true;
    </script>
<% } %>