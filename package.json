    "build": "bash paral 'npm run buildApp' 'npm run buildEmbed' 'npm run buildNative'",
    "buildApp": "cross-env BUILD_ENV=production node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js",
    "buildEmbed": "cross-env BUILD_ENV=production node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config webpackEmbed.config.js",
    "buildNative": "cross-env BUILD_ENV=production node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config webpackNative.config.js",
