    "devApp": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js",
    "devEmbed": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config webpackEmbed.config.js",
    "devNative": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config webpackNative.config.js",
