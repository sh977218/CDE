{
  "name": "ludetc-cdes",
  "version": "0.0.1",
  "main": "buildNode/app.js",
  "scripts": {
    "build": "npm run buildNode && bash paral 'npm run buildAppJs' 'npm run buildEmbedJs' 'npm run buildFhirJs' 'npm run buildNativeJs'",
    "buildApp": "npx ts-node -P tsconfigNode.json ./node_modules/webpack/bin/webpack.js --config ./webpackApp.prod.ts",
    "buildAppJs": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./buildNode/webpackApp.prod.js",
    "buildAppStats": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_app/webpackApp.prod.js --profile --json > compilation-stats.json",
    "buildAppStatsNative": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_nativeRenderApp/webpackNative.prod.js --profile --json > compilation-stats-native.json",
    "buildEmbed": "npx ts-node -P tsconfigNode.json ./node_modules/webpack/bin/webpack.js --config ./webpackEmbed.prod.ts",
    "buildEmbedJs": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./buildNode/webpackEmbed.prod.js",
    "buildFhir": "npx ts-node -P tsconfigNode.json ./node_modules/webpack/bin/webpack.js --config ./webpackFhir.prod.ts",
    "buildFhirJs": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./buildNode/webpackFhir.prod.js",
    "buildFnAwsJava": "cross-env cp -R modules/form/public/assets/sdc serverless-aws-java/src/main/resources/ && cd serverless-aws-java && chmod u+x gradlew && bash gradlew build && cd ..",
    "buildNative": "npx ts-node -P tsconfigNode.json ./node_modules/webpack/bin/webpack.js --config ./webpackNative.prod.ts && npx rollup dist/native/native-esm.js --file dist/native/native.js --format iife -c rollup.config.js",
    "buildNativeJs": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./buildNode/webpackNative.prod.js && npx rollup dist/native/native-esm.js --file dist/native/native.js --format iife -c rollup.config.js",
    "buildNode": "cross-env bash ./buildNode.sh",
    "bundle-report": "webpack-bundle-analyzer compilation-stats.json",
    "bundle-report-native": "webpack-bundle-analyzer compilation-stats-native.json",
    "coverage": "cross-env node ./node_modules/nyc/bin/nyc.js npm start",
    "dev": "npm run devApp",
    "devApp": "npx ts-node -P tsconfigNode.json ./node_modules/webpack/bin/webpack.js --config ./webpackApp.dev.ts",
    "devAppJs": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./buildNode/webpackApp.dev.js",
    "devEmbed": "npx ts-node -P tsconfigNode.json ./node_modules/webpack/bin/webpack.js --config ./webpackEmbed.dev.ts",
    "devEmbedJs": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./buildNode/webpackEmbed.dev.js",
    "devNative": "npx ts-node -P tsconfigNode.json ./node_modules/webpack/bin/webpack.js --config ./webpackNative.dev.ts",
    "devNativeJs": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./buildNode/webpackNative.dev.js",
    "devFhir": "npx ts-node -P tsconfigNode.json ./node_modules/webpack/bin/webpack.js --config ./webpackFhir.dev.ts",
    "devFhirJs": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./buildNode/webpackFhir.dev.js",
    "gulp": "npm run buildNode && npm run gulpJs",
    "gulpTs": "npx ts-node -P tsconfigNode.json ./node_modules/gulp/bin/gulp.js --gulpfile gulpfile.ts",
    "gulpJs": "npx gulp --gulpfile ./buildNode/gulpfile.js",
    "install": "napa",
    "moveMaterialCss": "cpx ./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css ./node_modules",
    "postinstall": "npm run moveMaterialCss",
    "start": "npm run startJs",
    "startJs": "node ./buildNode/app.js",
    "startJsWithBuild": "npm run buildNode && npm start",
    "startTs": "npx ts-node -P tsconfigNode.json app.ts",
    "ts": "npx ts-node -P tsconfigNode.json"
  },
  "engines": {
    "node": "0.12.x",
    "npm": "1.2.x"
  },
  "napa": {
    "feedback": "https://github.com/ivoviz/feedback.git#master"
