{
  "name": "ludetc-cdes",
  "version": "0.0.1",
  "main": "app.js",
  "scripts": {
    "build": "bash paral 'npm run buildApp' 'npm run buildEmbed' 'npm run buildFhir' 'npm run buildNative'",
    "buildApp": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_app/webpackApp.prod.js",
    "buildEmbed": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_embedApp/webpackEmbed.prod.js",
    "buildFhir": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_fhirApp/webpackFhir.prod.js",
    "buildFnAwsJava": "cp -R modules/form/public/assets/sdc serverless-aws-java/src/main/resources/ && cd serverless-aws-java && chmod u+x gradlew && ./gradlew build && cd ..",
    "buildNative": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_nativeRenderApp/webpackNative.prod.js && npx rollup dist/native/native-esm.js --file dist/native/native.js --format iife -c rollup.config.js",
    "dev": "npm run devApp",
    "devApp": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_app/webpackApp.dev.js",
    "devEmbed": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_embedApp/webpackEmbed.dev.js",
    "devNative": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_nativeRenderApp/webpackNative.dev.js",
    "devFhir": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_fhirApp/webpackFhir.dev.js",
    "install": "napa",
    "moveMaterialCss": "cpx ./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css ./node_modules",
    "postinstall": "npm run moveMaterialCss",
    "start": "node app.js",
    "buildAppStats": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_app/webpackApp.prod.js --profile --json > compilation-stats.json",
    "buildAppStatsNative": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_nativeRenderApp/webpackNative.prod.js --profile --json > compilation-stats-native.json",
    "bundle-report": "webpack-bundle-analyzer compilation-stats.json",
    "bundle-report-native": "webpack-bundle-analyzer compilation-stats-native.json"
  },
  "engines": {
    "node": "0.12.x",
    "npm": "1.2.x"
  },
  "napa": {
    "feedback": "https://github.com/ivoviz/feedback.git#master"
  },
  "nyc": {
    "check-coverage": true,
    "include": [
      "**/server/article/*.js",
      "**/server/log/*.js"
    ],
    "lines": "85",
    "branches": "75",
    "functions": "90",
    "reporter": "html"
