  "scripts": {
    "build": "bash paral 'npm run buildApp' 'npm run buildEmbed' 'npm run buildFhir' 'npm run buildNative'",
    "buildApp": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_app/webpack.prod.js",
    "buildEmbed": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_embedApp/webpackEmbed.prod.js",
    "buildNative": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_nativeRenderApp/webpackNative.prod.js",
    "buildFhir": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_fhirApp/webpackFhir.prod.js",
    "dev": "npm run devApp",
    "devApp": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_app/webpack.dev.js",
    "devEmbed": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_embedApp/webpackEmbed.dev.js",
    "devNative": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_nativeRenderApp/webpackNative.dev.js",
    "devFhir": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_fhirApp/webpackFhir.dev.js",
    "install": "napa",
    "moveMaterialCss": "cpx ./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css ./node_modules",
    "postinstall": "npm run moveMaterialCss",
    "start": "node app.js",
    "buildAppStats": "cross-env node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config ./modules/_app/webpack.prod.js --profile --json > compilation-stats.json",
    "bundle-report": "webpack-bundle-analyzer compilation-stats.json"
  },
