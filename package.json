{
  "name": "ludetc-cdes",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://ludetc@git-scm.nlm.nih.gov/scm/cde/cde.git"
  },
  "license": "",
  "main": "buildNode/server/app.js",
  "scripts": {
    "postinstall": "npx husky install",
    "ng": "ng",
    "build": "npm run buildNode && bash paral 'npm run buildApp' 'npm run buildNative'",
    "buildApp": "npx ng b cde-cli --configuration=development",
    "buildNative": "npx ng b nativeRender --configuration=development",
    "buildNode": "npx cross-env bash ./buildNode.sh",
    "buildIngester": "npx cross-env bash ./_buildIngester.sh",
    "coverage": "npx cross-env node ./node_modules/nyc/bin/nyc.js npm start",
    "dev": "npm run devApp",
    "devApp": "ng serve cde-cli",
    "devAppNr": "npx ng serve cde-cli --no-live-reload",
    "devNative": "npx ng serve nativeRender",
    "devNativeNr": "npx ng serve nativeRender --no-live-reload",
    "gulp": "npm run buildNode && npm run gulpJs",
    "gulpIngester": "npm run buildNode && npm run gulpJs -- --full",
    "gulpTs": "npx ts-node -P tsconfigNode.json ./node_modules/gulp/bin/gulp.js --gulpfile gulpfile.ts",
    "gulpJs": "npx gulp --gulpfile ./buildNode/gulpfile.js",
    "start": "npm run startJs",
    "start:coverage": "ng serve e2e-coverage -c playwright",
    "startJs": "node ./buildNode/server/app.js",
    "startJsWithBuild": "npm run buildNode && npm start",
    "startTs": "npx ts-node -P tsconfigNode.json server/app.ts",
    "testServer": "node ./test/testLoginServer.js",
    "ts": "npx ts-node -P tsconfigNode.json",
    "buildNodeIngester": "cross-env bash ./buildNodeIngester.sh",
    "lint": "ng lint cde-cli",
    "format": "prettier -w ./{modules,e2e-playwright}/**/*.ts",
    "esfix": "eslint --fix",
    "playwright": "playwright test -c e2e-playwright --project=CDE-e2e",
    "playwright:smokeTest": "playwright test -c e2e-playwright --project=CDE-smokeTest --headed",
    "playwright:smokeTest:debug": "playwright test -c e2e-playwright --project=CDE-smokeTest --headed --debug"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "exclude-after-remap": false,
    "include": [
      "**/app.ts",
      "**/server/article/*",
      "**/server/attachment/*",
      "**/server/board/*",
      "**/server/cde/*",
      "**/server/classification/*",
      "**/server/discuss/*",
      "**/server/form/*",
      "**/server/loader/*",
      "**/server/log/*",
      "**/server/nativeRender/*",
      "**/server/notification/*",
      "**/server/orgManagement/*",
      "**/server/siteAdmin/*",
      "**/server/user/*"
    ],
    "exclude": [
      "build/**",
      "**/server/form/aws*",
      "**/server/siteAdmin/status*",
      "**/server/form/syncLinkedForms*",
      "**/server/notification/notificationDb*"
    ],
    "lines": "90",
    "branches": "76",
    "functions": "91",
    "reporter": "html"
  },
