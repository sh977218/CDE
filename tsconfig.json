{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "allowUnreachableCode": true,
    "baseUrl": "./modules",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "listEmittedFiles": false,
    "listFiles": false,
    "module": "ES2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "pretty": true,
    "sourceMap": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@angular",
      "node_modules/@types"
    ]
  },
  "awesomeTypescriptLoaderOptions": {
    "useWebpackText": true
  },
  "exclude": [
    "build",
    "bower_components",
    "deploy",
    "dist",
    "ingester",
    "modules/components",
    "modules/static",
    "node_modules",
    "modules/system/node-js",
    "modules/cde/node-js",
    "modules/form/node-js",
    "modules/board/node-js",
    "scripts",
    "test"
  ],
  "angularCompilerOptions": {
    "skipMetadataEmit": true
  }
}
