{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "allowJs": false,
    "allowUnreachableCode": true,
    "baseUrl": "./modules",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "module": "ESNext",
    "moduleResolution": "Node",
    "pretty": false,
    "rootDir": ".",
    "sourceMap": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "target": "ES5",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "angularCompilerOptions": {
  },
  "awesomeTypescriptLoaderOptions": {
    "useWebpackText": true
  },
  "include": [
    "cde.d.ts",
    "modules/_app/main.ts",
    "modules/index.d.ts",
    "shared"
  ],
  "exclude": [
    "app.ts",
    "build",
    "buildNode",
    "buildWebStorm",
    "deploy",
    "dist",
    "gulpfile.ts",
    "ingester",
    "frontEnd/_embedApp",
    "frontEnd/_nativeRenderApp",
    "frontEnd/_fhirApp",
    "modules/form/public/assets",
    "node_modules",
    "protractor",
    "scripts",
    "server",
    "shared/node",
    "test",
    "webpack*"
  ]
}
