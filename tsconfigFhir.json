{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowJs": false,
    "allowUnreachableCode": true,
    "baseUrl": "./modules",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "module": "ES2015",
    "moduleResolution": "Node",
    "pretty": false,
    "rootDir": ".",
    "sourceMap": false,
    "strict": true,
    "target": "ES5",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "angularCompilerOptions": {
  },
  "awesomeTypescriptLoaderOptions": {
    "useWebpackText": true
  },
  "exclude": [
    "app.ts",
    "build",
    "buildNode",
    "deploy",
    "dist",
    "gulpfile.ts",
    "ingester",
    "modules/_app",
    "modules/_embedApp",
    "modules/_nativeRenderApp",
    "modules/adminItem",
    "modules/board",
    "modules/cde",
    "modules/classificationManagement",
    "modules/compare",
    "modules/discuss",
    "modules/embedded",
    "modules/form",
    "modules/home",
    "modules/non-core",
    "modules/processManager",
    "modules/quickBoard",
    "modules/search",
    "modules/siteAudit",
    "modules/settings",
    "modules/skipLogic",
    "modules/system",
    "modules/swagger",
    "modules/tag",
    "modules/usernameAutocomplete",
    "node_modules",
    "protractor",
    "scripts",
    "server",
    "test",
    "webpack*"
  ]
}
