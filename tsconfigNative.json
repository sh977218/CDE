{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowJs": false,
    "allowUnreachableCode": true,
    "baseUrl": "./modules",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "module": "ES2015",
    "moduleResolution": "Node",
    "pretty": false,
    "sourceMap": false,
    "strict": true,
    "target": "ES5",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "angularCompilerOptions": {
  },
  "awesomeTypescriptLoaderOptions": {
    "useWebpackText": true
  },
  "exclude": [
    "build",
    "deploy",
    "dist",
    "ingester",
    "modules/_app",
    "modules/_embedApp",
    "modules/_fhirApp",
    "modules/adminItem",
    "modules/board",
    "modules/cde",
    "modules/compare",
    "modules/core",
    "modules/discuss",
    "modules/embedded",
    "modules/form",
    "modules/home",
    "modules/processManager",
    "modules/quickBoard",
    "modules/search",
    "modules/system",
    "modules/swagger",
    "modules/inlineEdit",
    "modules/inlineAreaEdit",
    "modules/inlineSelectEdit",
    "modules/deleteWithConfirm",
    "modules/sortableArray",
    "modules/textTruncate",
    "modules/usernameAutocomplete",
    "node_modules",
    "protractor",
    "scripts",
    "server",
    "test"
  ]
}
