<h3><center>{{board.name}}<center></h3>

            
<div class="panel-group" id="accordion">
    <div class="panel panel-default" ng-repeat="cde in cdes">
        <div class="panel-heading">
            <a data-toggle="collapse" data-parent="#accordion" id="acc_head_{{$index}}" data-target="#acc_body_{{$index}}" href="">
                {{board.pins[$index].pinnedDate | date: 'MM/dd/yyyy @ h:mma'}} -- {{cde.naming[0].designation}}
            </a> --  <i class="fa fa-eye" ng-click="view(pin.cde)" title="View"> </i>
            <span ng-show="user._id">
                <i class="fa fa-thumb-tack fa-rotate-90" ng-click="unpin(board.pins[$index])" title="unPin" id="unpin_{{$index}}"> </i>
            </span>
        </div>
        <div id="acc_body_{{$index}}" class="panel-collapse collapse">
              <div class="panel-body">
                <div class="row">
                    <div class="col-md-6">
                        <%- include cdegeneraldetails.ejs %>
                    </div>
                    <div class="col-md-4">
                        <%- include valueDomainView.ejs %>
                    </div>
                     <div class="col-md-2">
                        <img ng-repeat="attachment in cde.attachments | filter:isDefaultAttachment" src="/data/{{attachment.fileid}}"/> 
                     </div>
                 </div>
                <div class="row" >
                    <a class='btn btn-small btn-default' ng-href="#/deview/?cdeId={{cde._id}}" title="Full Detail for {{cde.naming[0].designation}}"><i class="fa fa-eye"></i> View Full Detail</a>
                    <a class="btn btn-small btn-default" ng-show="showCompareButton(cde)" ng-click="addToCompareCart(cde._id)" title="Add to Compare List"><i class="fa fa-plus"></i> Compare</a>
                </div>
              </div>
        </div>
    </div>
</div>

<pagination align="center" direction-links="true" items-per-page="20" total-items="totalItems" page="currentPage"></pagination>

