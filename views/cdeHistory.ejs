<div class="row-fluid">
    <div class="span6">
    <table ng-show="priorCdes.length > 0" class="table-bordered table-condensed table-striped">
        <thead>
            <tr>
                <td>Updated at</td>
                <td>Updated by</td>
                <td>Change Note</td>
                <td>View Changes</td>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="priorCde in priorCdes">
                <td>{{priorCde.updated | date: 'MM/dd/yyyy @ h:mma'}}</td>
                <td>{{priorCde.updatedBy.username}}</td>
                <td>{{priorCde.changeNote}}</td>
                <td><i ng-show="priorCde.updated" class="icon-eye-open" ng-click="viewDiff(priorCde)"></i></td>
            </tr>
            <tr>
                <td>{{cde.updated | date: 'MM/dd/yyyy @ h:mma'}}</td>
                <td>{{cde.updatedBy.username}}</td>
                <td>{{cde.changeNote}}</td>
                <td><i class="icon-eye-open" ng-click="viewDiff(cde)"></i></td>
            </tr>
        </tbody>

    </table>
    <div ng-show="priorCdes.length == 0">This Data Element has no history</div>
    </div>
    <div>
        <dl>
            <dt ng-show="diff.name">Name: </dt><dd><span ng-bind-html="diff.name"></span></dd>
            <dt ng-show="diff.definition">Definition: </dt><dd><span ng-bind-html="diff.definition"></span></dd>
        </dl>
    </div>
</div>