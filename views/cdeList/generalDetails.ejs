<h4>General Details</h4>    
<dl class="dl-horizontal">
    <dt>Name: </dt>
    <dd id="dd_general_name" ng-bind-html="cde.primaryNameCopy">    

    </dd>    
        <dt>Steward: </dt><dd ng-bind-html="cde.stewardOrg.name"></dd>        
    <dt>Definition: </dt><dd id="dd_def" ng-bind-html="cde.primaryDefinitionCopy | placeholdEmpty"></dd>
    <dt>Used By: </dt><dd id="dd_usedBy">
        <span ng-switch="cde.classification.length > 0">
            <span ng-switch-when="false">None</span>
            <span ng-switch-default>
                <span ng-repeat="cl in cde.classification">{{cl.stewardOrg.name}} </span>
            </span>
        </span>
        </dd>
    <dt>Unit of Measure: </dt>
    <dd id="dd_uom">
        {{cde.valueDomain.uom | placeholdEmpty}}</dd>   
    <dt ng-show="cde.origin">Origin: </dt><dd>{{cde.origin}}</dd>
    <dt id="dt_status">Status: </dt><dd id="dd_status">{{cde.registrationState.registrationStatus}}</dd>
    <dt ng-show="cde.views">Views: </dt><dd id="dd_views">{{cde.views}}</dd>
</dl>
