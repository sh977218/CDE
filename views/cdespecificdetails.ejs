<dl class="dl-horizontal">
    <dt>Added to NLM: </dt><dd ng-controller="AuditCtrl" >{{cde.created | date: 'MM/dd/yyyy @ h:mma'}}</dd>
    <dt id="dt_updated" ng-show="cde.updated">Updated in NLM: </dt><dd id="dd_updated">{{cde.updated | date: 'MM/dd/yyyy @ h:mma'}}</dd>
    <dt id="dt_createdBy" ng-show="cde.createdBy">Created By: </dt><dd id="dd_createdBy">{{cde.createdBy.username}}</dd>
    <dt ng-show="cde.protocolDescription">Protocol Description: </dt> <dd>{{cde.protocolDescription}}</dd>
    <dt ng-show="cde.protocolText">Protocol Text </dt> <dd><div ng-bind-html="cde.protocolText"/></dd>
</dl>