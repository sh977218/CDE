<h4>Concepts</h4>
<table class="table-bordered table-condensed table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Origin</th>
            <th>Origin ID</th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="concept in cde.objectClass.concepts">
            <td>{{concept.name}}</td>
            <td>{{concept.origin}}</td>
            <td>{{concept.originId}}</td>
        </tr>
        <tr ng-repeat="concept in cde.property.concepts">
            <td>{{concept.name}}</td>
            <td>{{concept.origin}}</td>
            <td>{{concept.originId}}</td>
        </tr>
    </tbody>
</table>
