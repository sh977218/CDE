<div class="container-fluid">
    <div class="row">
    <div class="col-md-5">
    <form name="createCde" role="form">
        <fieldset>
            <legend>Create Data Element</legend>

            <div class="form-group">
                <label>Name</label>
                <input type="text" class="form-control" required ng-model="cde.designation" name="cde.designation" ng-change="showSuggestions()">
                <span ng-show="createCde.cde.designation.$error.required" >Required</span>
            </div>

            <div class="form-group">
                <label>Definition</label>
                <input type="text" class="form-control" required ng-model="cde.definition" name="cde.definition">
                <span ng-show="createCde.cde.definition.$error.required" class="help-inline">Required</span>
            </div>

            <div class="form-group">        
                <label>Version</label>
                <input type="text" class="form-control" ng-model="cde.version" name="cde.version">
            </div>

            <div class="form-group">
                <label for="cde.stewardOrg.name">Steward Organization</label>
                <select class="form-control" ng-model="cde.stewardOrg.name" name="cde.stewardOrg.name" ng-change="defaultClassification = []" required>
                    <option value="">Select One</option>
                    <option ng-repeat="o in myOrgs" ng-value="o">{{o}}</option>
                </select>
            </div>

            <div class="form-group" ng-show="cde.stewardOrg.name">
                <label for="selectDefault">Default Classification</label>
                <div ng-show="defaultClassification.length === 0">
                    <i class="fa fa-warning"></i> You must select a default classification.
                </div>
                <ol ng-show="defaultClassification" class="breadcrumb">
                    <li ng-repeat="cat in defaultClassification">{{cat}}</li>
                </ol>
                <button class="btn btn-default btn-small" ng-click="openSelectDefaultClassificationModal()" id="selectDefault">Select</button>
            </div>
            <script type="text/ng-template" id="selectDefaultClassificationModalContent.html">
                <%- include selectDefaultClassificationModal.ejs %>
            </script>
            <br />

            <a href="#/" class="btn btn-warning">Cancel</a>
            <input id="cde.submit" type="submit" ng-click="save()" class="btn btn-success" value="Save" ng-disabled="isClean() || createCde.$invalid || defaultClassification.length === 0"></input>
        </fieldset>
    </form>

    </div>
        <div class="col-md-7" ng-show="cdes">
            <h4>Possible Matches</h4>
            <div class="panel-group" id="accordion">
                <div class="panel panel-default" ng-repeat="cde in cdes" >
                    <div class="panel-heading">
                      <h3 class="panel-title">
                        <a data-toggle="collapse" data-parent="#accordion" id="acc_link_{{$index}}" data-target="#acc_body_{{$index}}" href="">
                            {{cde.naming[0].designation}}                    
                        </a>
                        <i class="fa fa-eye" ng-click="view(cde)" title="View"></i> 
                      </h3>
                    </div>
                    <div id="acc_body_{{$index}}" class="panel-collapse collapse">
                      <div class="panel-body">
                            <div class="row">
                               <div class="col-md-6">
                                   <%- include cdegeneraldetails.ejs %>
                               </div>
                               <div class="col-md-6">
                                   <%- include valueDomainView.ejs %>  
                               </div>
                             </div>
                             <div class="row" >
                               <a class='btn btn-default' ng-href="#/deview/?cdeId={{cde._id}}"><i class="fa fa-eye"></i> View Full Detail</a>
                             </div>
                      </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

