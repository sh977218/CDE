<div class="row">
<div class="col-md-5">
<form name="createCde" role="form">
    <fieldset>
        <legend>Create Data Element</legend>
        
        <div class="form-group">
            <label>Name</label>
            <input type="text" class="form-control" required ng-model="cde.designation" name="cde.designation" ng-change="showSuggestions()">
            <span ng-show="createCde.cde.designation.$error.required" class="help-inline">Required</span>
        </div>
        
        <div class="form-group">
            <label>Definition</label>
            <input type="text" class="form-control" required ng-model="cde.definition" name="cde.definition">
            <span ng-show="createCde.cde.definition.$error.required" class="help-inline">Required</span>
        </div>

        <div class="form-group">        
            <label>Version</label>
            <input type="text" class="form-control" ng-model="cde.version" name="cde.version">
        </div>

        <div class="form-group">
            <label for="cde.stewardOrg.name">Steward Organization</label>
            <select class="form-control" ng-model="cde.stewardOrg.name" name="cde.stewardOrg.name" required>
                <option value="">Select One</option>
                <option ng-repeat="o in myOrgs" ng-value="o">{{o}}</option>
            </select>
        </div>
        <br />
        

        <a href="#/" class="btn btn-warning">Cancel</a>
        <input id="cde.submit" type="submit" ng-click="save()" class="btn btn-success" value="Save" ng-disabled="isClean() || createCde.$invalid"></input>
    </fieldset>
</form>

</div>
    <div class="col-md-7" ng-show="cdes">
        <h4>Possible Matches</h4>
        <accordion>
            <accordion-group ng-repeat="cde in cdes" heading="{{cde.stewardOrg.name}} -- {{cde.naming[0].designation}}">
                 <div class="row">
                    <div class="col-md-6">
                        <%- include cdegeneraldetails.ejs %>
                    </div>
                    <div class="col-md-6">
                        <%- include valueDomainView.ejs %>  
                    </div>
                  </div>
                  <div class="row" >
                    <a class='btn' ng-href="#/deview/?cdeId={{cde._id}}"><i class="fa fa-eye"></i> View Full Detail</a>
                  </div>
            </accordion-group>
        </accordion>
    </div>
</div>

