<div ng-show="cde.<%- deConceptSource %>.concepts.length > 0">
<h5><%- deConceptName %></h5>
<table class="table-bordered table-condensed table-striped">
    <thead>
        <tr>
            <th>Code Name</th>
            <th>Code System</th>
            <th>Code ID</th>
            <th id="conceptActions" ng-show="isAllowed(cde)"></th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="concept in cde.<%- deConceptSource %>.concepts">
            <td><a ng-click="relatedCdes(concept)">{{concept.name}}</a></td>
            <td>{{concept.origin}}</td>
            <td>{{concept.originId}}</td>
            <td headers="actions" ng-show="isAllowed(cde)">
                 <a id="<%- deConceptId %>-{{$index}}" class="fa fa-trash-o" ng-click="<%- deConceptCallback %>($index)" title="Remove" href=""></a>
            </td>
        </tr>
    </tbody>
</table>
</div>