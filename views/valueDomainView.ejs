<table class="table-bordered table-condensed table-striped">
     <thead>
         <tr>
             <th>Name</th>
             <th>Code</th>
             <th>Code Name</th>
         </tr>
     </thead>
     <tbody>
         <tr ng-repeat="pv in cde.valueDomain.permissibleValues">
             <td>{{pv.validValue}}</td>
             <td>{{pv.valueCode}}</td>
             <td>{{pv.codeSystemName}}</td>
         </tr>
         <tr ng-show="cde.valueDomain.permissibleValues.length == 10">
             <td colspan="3">See full list in Detailed View</td>
         </tr>
     </tbody>
 </table>